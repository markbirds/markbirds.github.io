    "nuxt": "^3.17.1",
    "nuxt-swiper": "2.0.0",
    "tailwindcss": "^4.1.8",
    "typed.js": "^2.1.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.3.0",
    "@nuxtjs/stylelint-module": "^5.2.1",
    "@types/node": "^22.15.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
